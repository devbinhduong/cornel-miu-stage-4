{
  "components/common/navigation-menu": "<nav class=\"navPages\">\r\n    <ul class=\"navPages-list{{#if theme_settings.haloMegamenu}} navPages-list-megamenu{{/if}}{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\r\n        {{#each categories}}\r\n            <li class=\"navPages-item{{#if children}} has-dropdown{{/if}}\">\r\n                {{#if ../theme_settings.navigation_design '===' 'simple'}}\r\n                    {{> components/common/navigation-list}}\r\n                {{/if}}\r\n                {{#if ../theme_settings.navigation_design '===' 'alternate'}}\r\n                    {{> components/common/navigation-list-alternate}}\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n        {{#unless theme_settings.hide_content_navigation}}\r\n            {{#each pages}}\r\n                <li class=\"navPages-item navPages-item-page{{#if children}} has-dropdown{{/if}}\">\r\n                    {{> components/common/navigation-list-alternate}}\r\n                </li>\r\n            {{/each}}\r\n        {{/unless}}\r\n    </ul>\r\n</nav>\r\n",
  "components/common/navigation-list": "{{#if children}}\r\n<a class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\r\n   href=\"{{url}}\"\r\n   data-collapsible=\"navPages-{{id}}\"\r\n>\r\n    {{name}}\r\n    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\r\n        <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n    </i>\r\n</a>\r\n<div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\r\n    <ul class=\"navPage-subMenu-list\">\r\n        <li class=\"navPage-subMenu-item\">\r\n            <a class=\"navPage-subMenu-action navPages-action\"\r\n               href=\"{{url}}\"\r\n               aria-label=\"{{lang 'category.view_all.name' category=name}}\"\r\n            >\r\n                {{lang 'category.view_all.name' category=name}}\r\n            </a>\r\n        </li>\r\n        {{#each children}}\r\n            <li class=\"navPage-subMenu-item\">\r\n                {{#if children}}\r\n                    <a class=\"navPage-subMenu-action navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\r\n                       href=\"{{url}}\"\r\n                       aria-label=\"{{name}}\"\r\n                    >\r\n                        {{name}}\r\n                        <span class=\"collapsible-icon-wrapper\"\r\n                            data-collapsible=\"navPages-{{id}}\"\r\n                            data-collapsible-disabled-breakpoint=\"medium\"\r\n                            data-collapsible-disabled-state=\"open\"\r\n                            data-collapsible-enabled-state=\"closed\"\r\n                        >\r\n                            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                                <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                            </i>\r\n                        </span>\r\n                    </a>\r\n                    <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\r\n                        {{#each children}}\r\n                        <li class=\"navPage-childList-item\">\r\n                            <a class=\"navPage-childList-action navPages-action{{#if is_active}} activePage{{/if}}\"\r\n                               href=\"{{url}}\"\r\n                               aria-label=\"{{name}}\"\r\n                            >\r\n                                {{name}}\r\n                            </a>\r\n                        </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                {{else}}\r\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\"\r\n                       href=\"{{url}}\"\r\n                       aria-label=\"{{name}}\"\r\n                    >\r\n                        {{name}}\r\n                    </a>\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n</div>\r\n{{else}}\r\n<a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\r\n   href=\"{{url}}\"\r\n   aria-label=\"{{name}}\"\r\n>\r\n    {{name}}\r\n</a>\r\n{{/if}}\r\n",
  "components/common/navigation-list-alternate": "{{#if children}}\r\n    <p class=\"navPages-action navPages-action-lv1 navPages-action-depth-max has-subMenu is-root{{#if is_active}} activePage{{/if}}\" data-label=\"{{url}}\" data-index=\"{{@index}}\">\r\n        <a class=\"text {{name}}\" href=\"{{url}}\" aria-label=\"{{name}}\">\r\n          <span>{{name}}</span>\r\n        </a>\r\n        <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\r\n            <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n        </span>\r\n    </p>\r\n    {{> components/common/navigation-dropdown}}\r\n{{else}}\r\n    <a class=\"navPages-action navPages-action-lv1 no-subMenu navPages-action-end{{#if is_active}} activePage{{/if}}\" data-label=\"{{url}}\" href=\"{{url}}\" aria-label=\"{{name}}\" data-index=\"{{@index}}\">\r\n      <span class=\"text\">{{name}}</span>\r\n    </a>\r\n{{/if}}\r\n\r\n\r\n",
  "components/common/navigation-dropdown": "<div class=\"navPage-subMenu navPage-subMenu-horizontal level-2\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\r\n    <ul class=\"navPage-subMenu-list\">\r\n        <li class=\"navPage-subMenu-item-parent navPage-subMenu-item-child navPage-subMenu-title\">\r\n            <p class=\"navPage-subMenu-action navPages-action navPages-action-lv1 navPages-action-depth-max has-subMenu\">\r\n                <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                    <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                </span>\r\n                <a class=\"text\" href=\"{{url}}\"><span>{{name}}</span></a>\r\n            </p>\r\n        </li>\r\n        <li class=\"navPage-subMenu-item-child navPages-action-end navPage-subMenu-all\">\r\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-lv1 no-subMenu\" href=\"{{url}}\">\r\n                <span class=\"text\">{{lang 'category.view_all.name' category=name}}</span>\r\n            </a>\r\n        </li>\r\n        {{#each children}}\r\n            {{#if children}}\r\n                <li class=\"navPage-subMenu-item-child has-dropdown\">\r\n                    <p class=\"navPage-subMenu-action navPages-action navPages-action-lv2 navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" data-navId=\"navPages-{{id}}\">\r\n                        <a class=\"text\" href=\"{{url}}\"><span>{{name}}</span></a>\r\n                        <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                            <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                        </span>\r\n                    </p>\r\n                    <div class=\"navPage-subMenu navPage-subMenu-horizontal level-3\" aria-hidden=\"true\" tabindex=\"-1\">\r\n                        <ul class=\"navPage-subMenu-list\">\r\n                            <li class=\"navPage-subMenu-item-parent navPage-subMenu-item-child navPage-subMenu-title\">\r\n                                <p class=\"navPage-subMenu-action navPages-action navPages-action-lv2\">\r\n                                    <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                                        <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                                    </span>\r\n                                    <a class=\"text\" href=\"{{url}}\"><span>{{name}}</span></a>\r\n                                </p>\r\n                            </li>\r\n                            <li class=\"navPage-subMenu-item-child navPages-action-end navPage-subMenu-all\">\r\n                                <a class=\"navPage-subMenu-action navPages-action navPages-action-lv2 no-subMenu\" href=\"{{url}}\">\r\n                                    <span class=\"text\">{{lang 'category.view_all.name' category=name}}</span>\r\n                                </a>\r\n                            </li>\r\n                            {{#each children}}\r\n                                <li class=\"navPage-subMenu-item-child navPages-action-end\">\r\n                                    <a class=\"navPage-subMenu-action navPages-action navPages-action-lv3 no-subMenu level-3\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\r\n                                </li>\r\n                            {{/each}}\r\n                            <li class=\"navPage-subMenu-item-child navPages-action-end navPage-subMenu-all navPage-subMenu-all-custom-lv2\">\r\n                                <a class=\"navPage-subMenu-action navPages-action navPages-action-lv2 no-subMenu\" href=\"{{url}}\">\r\n                                    <span class=\"text\">{{lang 'category.view_all_menu'}}</span>\r\n                                </a>\r\n                            </li>\r\n                        </ul>\r\n                    </div>\r\n                </li>\r\n            {{else}}\r\n                <li class=\"navPage-subMenu-item-child navPages-action-end\">\r\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-lv2 no-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\r\n                </li>\r\n            {{/if}}\r\n        {{/each}}\r\n    </ul>\r\n</div>\r\n"
}