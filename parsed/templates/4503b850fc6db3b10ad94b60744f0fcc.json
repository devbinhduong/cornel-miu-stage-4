{
  "components/halothemes/product/halo-bundle-products-options": "{{#if product.options}}\r\n    <a class=\"button button--primary halo-toggle-options\" href=\"#\">\r\n        <span class=\"text\">{{lang 'products.choose_options'}}</span>\r\n    </a>\r\n    <div class=\"halo-detail-options halo-productOptions\" data-fbt-option-change>\r\n        <div class=\"halo-option-header\">\r\n            <span class=\"halo-option-title\">{{lang 'products.choose_options'}}</span>\r\n            <a href=\"#\" class=\"halo-option-close\" aria-label=\"Close\" role=\"button\">\r\n                <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.close'}}\">\r\n                    <title>{{lang 'common.close'}}</title>\r\n                    <desc>{{lang 'common.close'}}</desc>\r\n                    <use xlink:href=\"#icon-close\"></use>\r\n                </svg>\r\n            </a>\r\n        </div>\r\n        <div class=\"halo-option-content haloCustomScrollbar\">\r\n            {{#each product.options}}\r\n                {{{dynamicComponent 'components/halothemes/fbt-options'}}}\r\n            {{/each}}\r\n            <div class=\"productAttributes-message\">\r\n                <p class=\"alertBox-message\"></p>\r\n            </div>\r\n            {{#if theme_settings.halo_stock_level}}\r\n                <div class=\"productView-optionsStock u-hiddenVisually\">\r\n                    <span>{{lang 'products.stock_hurry_up'}}</span>\r\n                    <span data-stock-left></span>\r\n                </div>\r\n            {{/if}}\r\n        </div>\r\n    </div>\r\n{{/if}}\r\n",
  "components/halothemes/fbt-options/input-checkbox": "<div class=\"form-field\" data-product-attribute=\"input-checkbox\">\r\n    <label class=\"form-label form-label--inlineSmall\">\r\n        {{display_name}}\r\n\r\n        {{> components/common/requireness-msg}}\r\n    </label>\r\n    <input type=\"hidden\" name=\"attribute[{{id}}]\" value=\"{{noValue}}\" />\r\n    <input\r\n        class=\"form-checkbox\"\r\n        type=\"checkbox\"\r\n        name=\"attribute[{{id}}]\"\r\n        id=\"fbt_attribute-check-{{id}}\"\r\n        value=\"{{value}}\"\r\n        {{#if checked}}\r\n            checked\r\n            data-default\r\n        {{/if}}\r\n        {{#if required}}required{{/if}}>\r\n\r\n    <label class=\"form-label {{class}}\" for=\"fbt_attribute-check-{{id}}\">{{label}}</label>\r\n</div>\r\n",
  "components/halothemes/fbt-options/input-numbers": "<div class=\"form-field\" data-product-attribute=\"input-number\">\r\n    <label class=\"form-label form-label--inlineSmall\" for=\"fbt_attribute_number_{{id}}\">\r\n        {{ this.display_name }}\r\n\r\n        {{> components/common/requireness-msg}}\r\n    </label>\r\n\r\n    <input class=\"form-input\" type=\"number\" id=\"fbt_attribute_number_{{id}}\" {{#unless this.integer_only}}step=\"any\"{{/unless}} name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\r\n</div>\r\n",
  "components/halothemes/fbt-options/date": "<div class=\"form-field\" data-product-attribute=\"date\">\r\n    <label class=\"form-label form-label--inlineSmall\" {{#if style}}style=\"{{style}}\" {{/if}}>\r\n        {{this.display_name}}\r\n\r\n        {{> components/common/requireness-msg}}\r\n    </label>\r\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][month]\" {{#if required}}required{{/if}}>\r\n        <option value=\"\">{{lang 'common.month'}}</option>\r\n        {{#for 1 12}}\r\n            <option value=\"{{$index}}\" {{#if ../selected_date.month '==' $index}}selected=\"selected\"{{/if}}>\r\n                {{lang (concat 'common.short_months.' $index)}}\r\n            </option>\r\n        {{/for}}\r\n    </select>\r\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][day]\" {{#if required}}required{{/if}}>\r\n        <option value=\"\">{{lang 'common.day'}}</option>\r\n        {{#for 1 31}}\r\n            <option value=\"{{$index}}\" {{#if ../selected_date.day '==' $index}}selected=\"selected\"{{/if}}>\r\n                {{$index}}\r\n            </option>\r\n        {{/for}}\r\n    </select>\r\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][year]\" {{#if required}}required{{/if}}>\r\n        <option value=\"\">{{lang 'common.year'}}</option>\r\n        {{#for earliest_year latest_year}}\r\n            <option value=\"{{$index}}\" {{#if ../selected_date.year '==' $index}}selected=\"selected\"{{/if}}>\r\n                {{$index}}\r\n            </option>\r\n        {{/for}}\r\n        {{#if earliest_year '==' latest_year}}\r\n        <option value=\"{{earliest_year}}\" selected=\"selected\">{{earliest_year}}</option>\r\n        {{/if}}\r\n    </select>\r\n</div>\r\n",
  "components/halothemes/fbt-options/product-list": "<div class=\"form-field\" data-product-attribute=\"product-list\">\r\n    <label class=\"form-label form-label--inlineSmall\">\r\n        {{display_name}}\r\n        <span data-option-value></span>\r\n\r\n        {{> components/common/requireness-msg}}\r\n    </label>\r\n\r\n    {{#if values.0.image}}\r\n        <ul class=\"productOptions-list\">\r\n            {{#unless required}}\r\n                <li class=\"productOptions-list-item\">\r\n                    <input class=\"form-radio\"\r\n                           type=\"radio\"\r\n                           name=\"attribute[{{id}}]\"\r\n                           value=\"0\"\r\n                           id=\"fbt_attribute_productlist_0_{{id}}\"\r\n                           checked=\"{{#if defaultValue '==' 0}}checked{{/if}}\" required>\r\n                    <label class=\"form-label\" for=\"fbt_attribute_productlist_0_{{id}}\">{{lang 'products.none'}}</label>\r\n                </li>\r\n            {{/unless}}\r\n            {{#each values}}\r\n                <li class=\"productOptions-list-item\" data-product-attribute-value=\"{{id}}\">\r\n                    {{#if image}}\r\n                        <figure class=\"productOptions-list-item-figure\">\r\n                            <img class=\"productOptions-list-item-image lazyload\" data-sizes=\"auto\" src=\"{{#if ../theme_settings.halo_homepage_layout_1}}{{cdn 'img/loading.svg'}}{{else}}{{cdn 'img/loading_2.svg'}}{{/if}}\" data-src=\"{{getImage image 'productthumb_size'}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\r\n                        </figure>\r\n                    {{/if}}\r\n                    <div class=\"productOptions-list-item-content\">\r\n                        <input\r\n                            class=\"form-radio\"\r\n                            type=\"radio\"\r\n                            name=\"attribute[{{../id}}]\"\r\n                            value=\"{{id}}\"\r\n                            id=\"fbt_attribute_productlist_{{../id}}_{{id}}\"\r\n                            {{#if selected}}\r\n                                checked\r\n                                data-default\r\n                            {{/if}}\r\n                            {{#if ../required}}required{{/if}}>\r\n                        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"fbt_attribute_productlist_{{../id}}_{{id}}\">{{label}}</label>\r\n                    </div>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n    {{else}}\r\n        {{#unless required}}\r\n        <input class=\"form-radio\"\r\n               type=\"radio\"\r\n               name=\"attribute[{{id}}]\"\r\n               value=\"0\"\r\n               id=\"fbt_attribute_productlist_0_{{id}}\"\r\n               {{#if defaultValue '==' 0}}checked{{/if}}>\r\n        <label class=\"form-label\" for=\"fbt_attribute_productlist_0_{{id}}\">{{lang 'products.none'}}</label>\r\n        {{/unless}}\r\n        {{#each values}}\r\n            <input\r\n                class=\"form-radio\"\r\n                type=\"radio\"\r\n                name=\"attribute[{{../id}}]\"\r\n                value=\"{{id}}\"\r\n                id=\"fbt_attribute_productlist_{{../id}}_{{id}}\"\r\n                {{#if selected}}checked{{/if}}\r\n                {{#if ../required}}required{{/if}}>\r\n            <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"fbt_attribute_productlist_{{../id}}_{{id}}\">{{label}}</label>\r\n        {{/each}}\r\n    {{/if}}\r\n</div>\r\n",
  "components/halothemes/fbt-options/input-file": "<div class=\"form-field\" data-product-attribute=\"input-file\">\r\n    <label class=\"form-label form-label--inlineSmall\" for=\"fbt_attribute_file_{{id}}\">\r\n        {{ this.display_name }}\r\n\r\n        {{> components/common/requireness-msg}}\r\n    </label>\r\n\r\n    <input class=\"form-file\" type=\"file\" id=\"fbt_attribute_file_{{id}}\" name=\"attribute[{{this.id}}]\" {{#if required}}required{{/if}}>\r\n    {{#if original_name}}\r\n        {{{lang 'products.file_option_set' url=file_url name=original_name}}}\r\n        <label>{{lang 'cart.remove_file'}}<input type=\"checkbox\" name=\"{{file_remove}}\"></label>\r\n    {{/if}}\r\n    <p class=\"form-fileDescription\">{{#if file_size}}{{lang 'products.max_filesize'}} <strong>{{file_size}}</strong>, {{/if}}{{#if file_types}}{{lang 'products.filetypes'}} <strong>{{file_types}}</strong>{{/if}}</p>\r\n\r\n</div>\r\n",
  "components/halothemes/fbt-options/set-rectangle": "<div class=\"form-field\" data-product-attribute=\"set-rectangle\">\r\n    <label class=\"form-label form-label--inlineSmall\">\r\n        {{this.display_name}}\r\n        {{> components/common/requireness-msg}}\r\n        <span data-option-value></span>\r\n    </label>\r\n    {{#each this.values}}\r\n        <input\r\n            class=\"form-radio\"\r\n            type=\"radio\"\r\n            id=\"fbt_attribute_rectangle__{{../id}}_{{id}}\"\r\n            name=\"attribute[{{../id}}]\"\r\n            value=\"{{id}}\"\r\n            {{#if selected}}\r\n                checked\r\n                data-default\r\n            {{/if}}\r\n            {{#if ../required}}required{{/if}}>\r\n        <label class=\"form-option form-option-rectangle\" for=\"fbt_attribute_rectangle__{{../id}}_{{id}}\" data-product-attribute-value=\"{{id}}\">\r\n            <span class=\"form-option-variant\"><span>{{this.label}}</span></span>\r\n        </label>\r\n    {{/each}}\r\n</div>\r\n",
  "components/halothemes/fbt-options/set-radio": "<div class=\"form-field\" data-product-attribute=\"set-radio\">\r\n    <label class=\"form-label form-label--inlineSmall\">\r\n        {{ this.display_name }}\r\n\r\n        {{> components/common/requireness-msg}}\r\n        <span data-option-value></span>\r\n    </label>\r\n\r\n    {{#each this.values}}\r\n        <input\r\n            class=\"form-radio\"\r\n            type=\"radio\"\r\n            id=\"fbt_attribute_radio_{{../id}}_{{id}}\"\r\n            name=\"attribute[{{../id}}]\"\r\n            value=\"{{id}}\"\r\n            {{#if selected}}\r\n                checked\r\n                data-default\r\n            {{/if}}\r\n            {{#if ../required}}required{{/if}}>\r\n        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"fbt_attribute_radio_{{../id}}_{{id}}\">{{this.label}}</label>\r\n    {{/each}}\r\n</div>\r\n",
  "components/halothemes/fbt-options/textarea": "<div class=\"form-field\" data-product-attribute=\"textarea\">\r\n    <label class=\"form-label form-label--inlineSmall\" for=\"fbt_attribute_textarea_{{id}}\">\r\n        {{display_name}}\r\n\r\n        {{> components/common/requireness-msg}}\r\n    </label>\r\n    <textarea class=\"form-input\" id=\"fbt_attribute_textarea_{{id}}\" name=\"attribute[{{id}}]\" {{#if required}}required{{/if}} {{#if max_length}}maxlength={{max_length}}{{/if}}>{{prefill}}</textarea>\r\n</div>\r\n",
  "components/halothemes/fbt-options/input-text": "<div class=\"form-field\" data-product-attribute=\"input-text\">\r\n    <label class=\"form-label form-label--inlineSmall\" for=\"fbt_attribute_text_{{id}}\">\r\n        {{ this.display_name }}\r\n\r\n        {{> components/common/requireness-msg}}\r\n    </label>\r\n\r\n    <input class=\"form-input form-input--small\" type=\"text\" id=\"fbt_attribute_text_{{id}}\" name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\r\n</div>\r\n",
  "components/halothemes/fbt-options/set-select": "<div class=\"form-field\" data-product-attribute=\"set-select\">\r\n    <label class=\"form-label form-label--inlineSmall\" for=\"fbt_attribute_select_{{id}}\">\r\n        {{ this.display_name }}\r\n\r\n        {{> components/common/requireness-msg}}\r\n        <span data-option-value></span>\r\n    </label>\r\n\r\n    <select class=\"form-select form-select--small\" name=\"attribute[{{this.id}}]\" id=\"fbt_attribute_select_{{id}}\" {{#if required}}required{{/if}}>\r\n        <option value=\"\">{{lang 'products.choose_options'}}</option>\r\n        {{#each this.values}}\r\n            <option data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" {{#if selected}}selected data-default{{/if}}>{{label}}</option>\r\n        {{/each}}\r\n    </select>\r\n</div>\r\n",
  "components/halothemes/fbt-options/swatch": "<div class=\"form-field\" data-product-attribute=\"swatch\">\r\n    <label class=\"form-label form-label--inlineSmall\">\r\n        {{this.display_name}}\r\n        {{> components/common/requireness-msg}}\r\n        <span data-option-value></span>\r\n    </label>\r\n\r\n    {{#each this.values}}\r\n        <input class=\"form-radio\"\r\n           type=\"radio\"\r\n           name=\"attribute[{{../id}}]\"\r\n           value=\"{{id}}\"\r\n           id=\"fbt_attribute_swatch_{{../id}}_{{id}}\"\r\n           {{#if selected}}checked data-default{{/if}}\r\n           {{#if ../required}}required{{/if}}\r\n           aria-label=\"{{this.label}}\"\r\n        >\r\n        <label class=\"form-option form-option-swatch\" for=\"fbt_attribute_swatch_{{../id}}_{{id}}\" data-product-attribute-value=\"{{id}}\">\r\n            {{#if image}}\r\n                <span class='form-option-variant form-option-variant--pattern' title=\"{{this.label}}\" style=\"background-image: url('{{getImage image \"swatch_option_size\"}}');\"></span>\r\n            {{/if}}\r\n            {{#if data.[2]}}\r\n                <span class='form-option-variant form-option-variant--color form-option-variant--color2' title=\"{{this.label}}\">\r\n                    <span style=\"background-color: #{{data.[0]}}\"></span>\r\n                    <span style=\"background-color: #{{data.[1]}}\"></span>\r\n                    <span style=\"background-color: #{{data.[2]}}\"></span>\r\n                </span>\r\n            {{else}}\r\n                {{#if data.[1]}}\r\n                    <span class='form-option-variant form-option-variant--color form-option-variant--color2' title=\"{{this.label}}\">\r\n                        <span style=\"background-color: #{{data.[0]}}\"></span>\r\n                        <span style=\"background-color: #{{data.[1]}}\"></span>\r\n                    </span>\r\n                {{else}}\r\n                    {{#if data.[0]}}\r\n                        <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\r\n                    {{/if}}\r\n                {{/if}}\r\n            {{/if}}\r\n            {{#if pattern}}\r\n                <span class=\"form-option-expanded\">\r\n                    <span class=\"form-option-image\" style=\"background-image: url('{{getImage image 'core-swatch'}}');\"></span>\r\n                </span>\r\n            {{/if}}\r\n        </label>\r\n    {{/each}}\r\n</div>\r\n",
  "components/common/requireness-msg": "<small>\r\n    {{#if required}}*{{/if}}\r\n</small>\r\n"
}