{
  "components/products/options/set-rectangle": "<div class=\"form-field\" data-product-attribute=\"set-rectangle\" data-product-option-id=\"{{id}}\" role=\"radiogroup\" aria-labelledby=\"rectangle-group-label\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" id=\"rectangle-group-label\">\r\n        {{this.display_name}}\r\n        {{> components/common/requireness-msg}}\r\n        <span data-option-value></span>\r\n    </label>\r\n\r\n    {{#unless required}}\r\n        <input\r\n            class=\"form-radio\"\r\n            type=\"radio\"\r\n            id=\"attribute_rectangle__{{../id}}_none\"\r\n            name=\"attribute[{{../id}}]\"\r\n            value=\"\"\r\n            checked=\"{{#if defaultValue '==' ''}}checked{{/if}}\"\r\n            data-title=\"{{this.label}}\"\r\n            data-index=\"{{@index}}\"\r\n        >\r\n        <label class=\"form-option\" for=\"attribute_rectangle__{{../id}}_none\">\r\n            <span class=\"form-option-variant\">{{lang 'products.none'}}</span>\r\n        </label>\r\n    {{/unless}}\r\n\r\n    <div class=\"option-value-size\">\r\n        {{#each this.values}}\r\n            <input data-filter=\".filter-{{camelcase this.label}}\"\r\n                class=\"form-radio\"\r\n                type=\"radio\"\r\n                id=\"attribute_rectangle__{{../id}}_{{id}}\"\r\n                name=\"attribute[{{../id}}]\"\r\n                value=\"{{id}}\"\r\n                {{#if selected}}\r\n                    checked\r\n                    data-default\r\n                {{/if}}\r\n                {{#if ../required}}required{{/if}}\r\n                data-title=\"{{this.label}}\"\r\n                data-index=\"{{@index}}\"\r\n            >\r\n            <label class=\"form-option form-option-rectangle\" for=\"attribute_rectangle__{{../id}}_{{id}}\" data-product-attribute-value=\"{{id}}\">\r\n                <span class=\"form-option-variant\"><span>{{this.label}}</span></span>\r\n            </label>\r\n        {{/each}}\r\n    </div>\r\n</div>\r\n",
  "components/common/requireness-msg": "<small>\r\n    {{#if required}}*{{/if}}\r\n</small>\r\n"
}