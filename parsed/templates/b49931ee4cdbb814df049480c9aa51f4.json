{
  "components/products/reviews": "<div class=\"productView-reviews-wrapper productReviews\">\r\n    {{#if reviews.total}}\r\n        <div class=\"productReview-total\">\r\n            <span>\r\n                {{lang 'products.reviews.link_to_review' total=reviews.total}}\r\n            </span>\r\n        </div>\r\n        <ul class=\"productReviews-list\" id=\"productReviews-list\">\r\n            {{#each reviews.list}}\r\n                <li class=\"productReview\">\r\n                    <article>\r\n                        <header>\r\n                            <h5 class=\"productReview-title\">\r\n                                <span class=\"productReview-title-text\">{{{ sanitize title }}}</span>\r\n                                <span class=\"productReview-rating rating--small\">\r\n                                    {{> components/products/ratings rating=rating}}\r\n                                    <span class=\"productReview-ratingNumber\">{{ rating }}</span>\r\n                                </span>\r\n                            </h5>\r\n                            {{#if name}}\r\n                                <meta content=\"{{name}}\">\r\n                                <p class=\"productReview-author\">\r\n                                    {{lang 'products.reviews.post_on_by' date=date name=name }}\r\n                                </p>\r\n                            {{else}}\r\n                                <p class=\"productReview-author\">\r\n                                    {{lang 'products.reviews.post_on_by' date=date name=(lang 'products.reviews.anonymous_poster')}}\r\n                                </p>\r\n                            {{/if}}\r\n                        </header>\r\n                        <p class=\"productReview-body\">{{{ sanitize text }}}</p>\r\n                    </article>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n        {{#or pagination.reviews.next pagination.reviews.previous}}\r\n            {{> components/common/paginator pagination.reviews}}\r\n        {{/or}}\r\n        <div class=\"button-createReview\">\r\n            <a class=\"button button--primary\" href=\"#\" data-reveal-id=\"modal-review-form\">{{lang 'products.reviews.write_a_review'}}</a>\r\n        </div>\r\n    {{else}}\r\n        <div class=\"alertBox alertBox--warning\">\r\n            <div class=\"alertBox-column alertBox-icon\">\r\n                <svg class=\"icon\"><use xlink:href=\"#icon-info\"></use></svg>\r\n            </div>\r\n            <p class=\"alertBox-column alertBox-message\">\r\n                {{lang 'products.reviews.write_a_review_text'}}\r\n            </p>\r\n        </div>\r\n        <div class=\"button-createReview\">\r\n            <a class=\"button button--primary\" href=\"#\" data-reveal-id=\"modal-review-form\">{{lang 'products.reviews.write_a_review'}}</a>\r\n        </div>\r\n    {{/if}}\r\n</div>\r\n",
  "components/common/paginator": "<nav class=\"pagination{{#if theme_settings.halo-product-show-more}} pagination-2{{/if}}\" aria-label=\"pagination\">\r\n    {{#if page_type '==' 'category'}}\r\n        {{#if total}}\r\n            {{#if start}}\r\n                {{#if end}}\r\n                    {{#if end '<' total}}\r\n                        <div class=\"pagination-info\"><span class=\"end\">{{end}}</span> din <span class=\"total\">{{total}}</span> de carti</div>\r\n                        <div class=\"pagination-total-progress\">\r\n                            <span class=\"pagination-total-item\"></span>\r\n                        </div>\r\n                    {{else}}\r\n                        <div class=\"pagination-info\"><span class=\"end\">{{end}}</span> din <span class=\"total\">{{total}}</span> de carti</div>\r\n                        <div class=\"pagination-total-progress\">\r\n                            <span class=\"pagination-total-item\"></span>\r\n                        </div>\r\n                    {{/if}}\r\n                {{/if}}\r\n            {{/if}}\r\n        {{/if}}\r\n        {{#if theme_settings.halo-product-show-more}}\r\n            <div class=\"halo-product-show-more\" id=\"listing-showmoreBtn\">\r\n                {{#if next}}\r\n                    <a class=\"button button--primary\" href=\"#\">\r\n                        <span class=\"text\">{{lang 'search.faceted.show-more'}}</span>\r\n                    </a>\r\n                {{else}}\r\n                    <a class=\"button button--primary disable\" href=\"#\">\r\n                        <span class=\"text\">{{lang 'common.no_more_products'}}</span>\r\n                    </a>\r\n                {{/if}}\r\n            </div> \r\n        {{/if}}\r\n    {{/if}}\r\n    {{#if page_type '==' 'brand'}}\r\n        {{#if links.length}}\r\n            {{#each shop_by_brand}}\r\n                {{#if name '==' ../brand.name}}\r\n                    <div class=\"pagination-info\">Items <span class=\"start\"></span> to <span class=\"end\"></span> of <span class=\"total\">{{count}}</span> total</div>\r\n                    <div class=\"pagination-total-progress\">\r\n                        <span class=\"pagination-total-item\"></span>\r\n                    </div>\r\n                {{/if}}\r\n            {{/each}}\r\n        {{/if}}\r\n        {{#if theme_settings.halo-product-show-more}}\r\n            <div class=\"halo-product-show-more\" id=\"listing-showmoreBtn\">\r\n                {{#if next}}\r\n                    <a class=\"button button--primary\" href=\"#\">\r\n                        <span class=\"text\">{{lang 'search.faceted.show-more'}}</span>\r\n                    </a>\r\n                {{else}}\r\n                    <a class=\"button button--primary disable\" href=\"#\">\r\n                        <span class=\"text\">{{lang 'common.no_more_products'}}</span>\r\n                    </a>\r\n                {{/if}}\r\n            </div> \r\n        {{/if}}\r\n    {{/if}}\r\n    {{#if page_type '==' 'search'}}\r\n        <div class=\"pagination-info\">Items <span class=\"start\"></span> to <span class=\"end\"></span> of <span class=\"total\">{{total}}</span> total</div>\r\n        <div class=\"pagination-total-progress\">\r\n            <span class=\"pagination-total-item\"></span>\r\n        </div>\r\n        {{#if theme_settings.halo-product-show-more}}\r\n            <div class=\"halo-product-show-more\" id=\"listing-showmoreBtn\">\r\n                {{#if next}}\r\n                    <a class=\"button button--primary\" href=\"#\">\r\n                        <span class=\"text\">{{lang 'search.faceted.show-more'}}</span>\r\n                    </a>\r\n                {{else}}\r\n                    <a class=\"button button--primary disable\" href=\"#\">\r\n                        <span class=\"text\">{{lang 'common.no_more_products'}}</span>\r\n                    </a>\r\n                {{/if}}\r\n            </div> \r\n        {{/if}}\r\n    {{/if}}\r\n    <ul class=\"pagination-list\">\r\n        {{#if previous}}\r\n            <li class=\"pagination-item pagination-item--previous\">\r\n                <a class=\"pagination-link\"\r\n                   href=\"{{previous}}\"\r\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\r\n                   aria-label=\"{{lang 'common.previous'}}\"\r\n                >\r\n                    <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"8\" height=\"17\" viewBox=\"0 0 8 17\" fill=\"none\">\r\n                      <path fill-rule=\"evenodd\" clip-rule=\"evenodd\" d=\"M6.19937 8.49981L0.511719 1.39024L1.4878 0.609375L7.4878 8.10937L7.4878 8.89024L1.48781 16.3902L0.511719 15.6094L6.19937 8.49981Z\" fill=\"#252525\"/>\r\n                    </svg>\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n\r\n        {{#each links}}\r\n            <li class=\"pagination-item {{#if this.number '==' ../current}}pagination-item--current{{/if}}\">\r\n                <a class=\"pagination-link\"\r\n                   href=\"{{this.url}}\"\r\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\r\n                   {{#if this.number '==' ../current}}aria-current=\"page\"{{/if}}\r\n                   aria-label=\"{{lang 'common.paginator.page_of' current=this.number total=../links.length}}\"\r\n                >\r\n                    {{this.number}}\r\n                </a>\r\n            </li>\r\n        {{/each}}\r\n\r\n        {{#if next}}\r\n            <li class=\"pagination-item pagination-item--next\">\r\n                <a class=\"pagination-link\"\r\n                   href=\"{{next}}\"\r\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\r\n                   aria-label=\"{{lang 'common.next'}}\"\r\n                >\r\n                    <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"8\" height=\"17\" viewBox=\"0 0 8 17\" fill=\"none\">\r\n                      <path fill-rule=\"evenodd\" clip-rule=\"evenodd\" d=\"M6.19937 8.49981L0.511719 1.39024L1.4878 0.609375L7.4878 8.10937L7.4878 8.89024L1.48781 16.3902L0.511719 15.6094L6.19937 8.49981Z\" fill=\"#252525\"/>\r\n                    </svg>\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n    </ul>\r\n</nav>\r\n",
  "components/products/ratings": "<span role=\"img\"\r\n    {{#if rating_target}}\r\n      aria-label=\"{{lang 'products.reviews.rating_aria_label' rating_target=rating_target current_rating=rating max_rating=5}}\"\r\n    {{else}}\r\n      aria-label=\"{{lang 'products.reviews.rating_aria_label' rating_target='Product' current_rating=rating max_rating=5}}\"\r\n    {{/if}}\r\n>\r\n    {{#for 1 5}}\r\n        {{#if ../this.rating '>=' $index}}\r\n            <span class=\"icon icon--ratingFull\">\r\n                <svg>\r\n                    <use xlink:href=\"#icon-star\" />\r\n                </svg>\r\n            </span>\r\n        {{else}}\r\n            <span class=\"icon icon--ratingEmpty\">\r\n                <svg>\r\n                    <use xlink:href=\"#icon-star\" />\r\n                </svg>\r\n            </span>\r\n        {{/if}}\r\n    {{/for}}\r\n</span>\r\n"
}