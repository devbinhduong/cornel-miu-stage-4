  "components/halothemes/sidebar/halo-side-block": "{{#if page_type '!=' 'login'}}\r\n\t{{#if customer}}\r\n\t\t<div id=\"halo-auth-sidebar\" class=\"halo-sidebar halo-auth-sidebar halo-sidebar-right\">\r\n\t\t\t<div class=\"halo-sidebar-header\">\r\n\t\t\t\t<span class=\"title\">{{lang 'account.title'}}</span>\r\n\t\t\t\t<a href=\"#\" class=\"close\">\r\n\t\t\t\t\t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.close'}}\">\r\n\t\t            \t<title>{{lang 'common.close'}}</title>\r\n\t\t            \t<desc>{{lang 'common.close'}}</desc>\r\n\t\t            \t<use xlink:href=\"#icon-close\"></use>\r\n\t\t            </svg>\r\n\t\t\t\t</a>\r\n\t\t\t</div>\r\n\t\t    <div class=\"halo-sidebar-wrapper\">\r\n\t\t        <ul class=\"account-dropdown account-dropdown-sidebar\">\r\n\t\t            <li class=\"account-item\">\r\n\t\t                <a class=\"account-action\" href=\"{{urls.account.orders.all}}\">\r\n\t\t                    <span class=\"text\">\r\n\t\t                    \t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.nav.orders'}}\">\r\n\t                                <title>{{lang 'account.nav.orders'}}</title>\r\n\t                                <desc>{{lang 'account.nav.orders'}}</desc>\r\n\t                                <use xlink:href=\"#icon-order\"></use>\r\n\t                            </svg>\r\n\t\t                    \t{{lang 'account.nav.orders'}}\r\n\t\t                    </span>\r\n\t\t                </a>\r\n\t\t            </li>\r\n\t\t            {{#if settings.returns_enabled}}\r\n\t\t                <li class=\"account-item\">\r\n\t\t                    <a class=\"account-action\" href=\"{{urls.account.returns}}\">\r\n\t\t                        <span class=\"text\">\r\n\t\t                        \t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.nav.returns'}}\">\r\n\t\t                                <title>{{lang 'account.nav.returns'}}</title>\r\n\t\t                                <desc>{{lang 'account.nav.returns'}}</desc>\r\n\t\t                                <use xlink:href=\"#icon-return\"></use>\r\n\t\t                            </svg>\r\n\t\t\t                    \t{{lang 'account.nav.returns'}}\r\n\t\t                        </span>\r\n\t\t                    </a>\r\n\t\t                </li>\r\n\t\t            {{/if}}\r\n\t\t            <li class=\"account-item\">\r\n\t\t                <a class=\"account-action\" href=\"{{urls.account.inbox}}\">\r\n\t\t                    <span class=\"text\">\r\n\t\t                    \t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.mobile_nav.messages'}}\">\r\n\t                                <title>{{lang 'account.mobile_nav.messages'}}</title>\r\n\t                                <desc>{{lang 'account.mobile_nav.messages'}}</desc>\r\n\t                                <use xlink:href=\"#icon-message\"></use>\r\n\t                            </svg>\r\n\t\t                    \t{{lang 'account.mobile_nav.messages'}}\r\n\t\t                    </span>\r\n\t\t                </a>\r\n\t\t            </li>\r\n\t\t             <li class=\"account-item\">\r\n\t\t                <a class=\"account-action\" href=\"{{urls.account.addresses}}\">\r\n\t\t                    <span class=\"text\">\r\n\t\t                    \t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.nav.addresses'}}\">\r\n\t                                <title>{{lang 'account.nav.addresses'}}</title>\r\n\t                                <desc>{{lang 'account.nav.addresses'}}</desc>\r\n\t                                <use xlink:href=\"#icon-address-book\"></use>\r\n\t                            </svg>\r\n\t\t                    \t{{lang 'account.nav.addresses'}}\r\n\t\t                    </span>\r\n\t\t                </a>\r\n\t\t            </li>\r\n\t\t            {{#if settings.show_payment_methods}}\r\n\t\t                <li class=\"account-item\">\r\n\t\t                    <a class=\"account-action\" href=\"{{urls.account.payment_methods.all}}\">\r\n\t\t                        <span class=\"text\">\r\n\t\t                        \t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.nav.payment_methods'}}\">\r\n\t\t                                <title>{{lang 'account.nav.payment_methods'}}</title>\r\n\t\t                                <desc>{{lang 'account.nav.payment_methods'}}</desc>\r\n\t\t                                <use xlink:href=\"#icon-payment\"></use>\r\n\t\t                            </svg>\r\n\t\t\t                    \t{{lang 'account.nav.payment_methods'}}\r\n\t\t                        </span>\r\n\t\t                    </a>\r\n\t\t                </li>\r\n\t\t            {{/if}}\r\n\t\t            {{#if settings.show_wishlist}}\r\n\t\t                <li class=\"account-item\">\r\n\t\t                    <a class=\"account-action\" href=\"{{urls.account.wishlists.all}}\">\r\n\t\t                        <span class=\"text\">\r\n\t\t                        \t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.wishlists.heading'}}\">\r\n\t                                    <title>{{lang 'account.wishlists.heading'}}</title>\r\n\t                                    <desc>{{lang 'account.wishlists.heading'}}</desc>\r\n\t                                    <use xlink:href=\"#icon-heart\"></use>\r\n\t                                </svg>\r\n\t\t                        \t{{lang 'account.mobile_nav.wishlists'}}\r\n\t\t                        </span>\r\n\t\t                    </a>\r\n\t\t                </li>\r\n\t\t            {{/if}}\r\n\t\t            <li class=\"account-item\">\r\n\t\t                <a class=\"account-action\" href=\"{{urls.account.recent_items}}\">\r\n\t\t                    <span class=\"text\">\r\n\t\t                    \t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.nav.recently_viewed'}}\">\r\n\t                                <title>{{lang 'account.nav.recently_viewed'}}</title>\r\n\t                                <desc>{{lang 'account.nav.recently_viewed'}}</desc>\r\n\t                                <use xlink:href=\"#icon-recently-viewed\"></use>\r\n\t                            </svg>\r\n\t\t                    \t{{lang 'account.nav.recently_viewed'}}\r\n\t\t                    </span>\r\n\t\t                </a>\r\n\t\t            </li>\r\n\t\t            <li class=\"account-item\">\r\n\t\t                <a class=\"account-action\" href=\"{{urls.account.details}}\">\r\n\t\t                    <span class=\"text\">\r\n\t\t                    \t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.nav.settings'}}\">\r\n\t                                <title>{{lang 'account.nav.settings'}}</title>\r\n\t                                <desc>{{lang 'account.nav.settings'}}</desc>\r\n\t                                <use xlink:href=\"#icon-account-settings\"></use>\r\n\t                            </svg>\r\n\t\t                    \t{{lang 'account.nav.settings'}}\r\n\t\t                    </span>\r\n\t\t                </a>\r\n\t\t            </li>\r\n\t\t            <li class=\"account-item\">\r\n\t\t                <a class=\"account-action\" href=\"{{urls.auth.logout}}\">\r\n\t\t                    <span class=\"text\">\r\n\t\t                    \t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.logout'}}\">\r\n\t                                <title>{{lang 'common.logout'}}</title>\r\n\t                                <desc>{{lang 'common.logout'}}</desc>\r\n\t                                <use xlink:href=\"#icon-sign-out\"></use>\r\n\t                            </svg>\r\n\t\t                    \t{{lang 'common.logout'}}\r\n\t\t                    </span>\r\n\t\t                </a>\r\n\t\t            </li>\r\n\t\t        </ul>\r\n\t\t    </div>\r\n\t\t</div>\r\n\t{{else}}\r\n\t\t<div id=\"halo-auth-sidebar\" class=\"halo-sidebar login-form-sidebar halo-auth-sidebar halo-sidebar-right\">\r\n\t\t\t<div class=\"halo-sidebar-header\">\r\n\t\t\t\t<span class=\"title\">{{lang 'login.title'}}</span>\r\n\t\t\t\t<a href=\"#\" class=\"close\">\r\n\t\t\t\t\t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.close'}}\">\r\n\t\t\t        \t<title>{{lang 'common.close'}}</title>\r\n\t\t\t        \t<desc>{{lang 'common.close'}}</desc>\r\n\t\t\t        \t<use xlink:href=\"#icon-close\"></use>\r\n\t\t\t        </svg>\r\n\t\t\t\t</a>\r\n\t\t\t</div>\r\n\t\t    <div class=\"halo-sidebar-wrapper\">\r\n\t\t        <form class=\"form\" action=\"{{settings.secure_base_url}}/login.php?action=check_login\" method=\"post\">\r\n\t\t            <div class=\"login-form-wrapper\">\r\n\t\t                <div class=\"form-field\">\r\n\t\t                    <label class=\"form-label\" for=\"login_email\">\r\n\t\t                        {{lang 'login.field_email'}}\r\n\t\t                        <small>*</small>\r\n\t\t                    </label>\r\n\t\t                    <input id=\"login_email\" class=\"form-input\" type=\"email\" value=\"\" name=\"login_email\" placeholder=\"john.smith@example.com\">\r\n\t\t                </div>\r\n\t\t                <div class=\"form-field\">\r\n\t\t                    <label class=\"form-label\" for=\"login_pass\">\r\n\t\t                        {{lang 'login.field_password'}}\r\n\t\t                        <small>*</small>\r\n\t\t                    </label>\r\n\t\t                    <input id=\"login_pass\" class=\"form-input\" type=\"password\" value=\"\" name=\"login_pass\" placeholder=\"\">\r\n\t\t                </div>\r\n\t\t                <div class=\"form-actions\">\r\n\t\t                    <button type=\"button\" class=\"button button--tertiary\">{{lang 'login.submit_value'}}</button>\r\n\t\t                    <a class=\"forgot-password\" href=\"{{urls.auth.forgot_password}}\">\r\n\t\t                    \t<span>{{lang 'login.forgot_password'}}</span>\r\n\t\t                    </a>\r\n\t\t                    <a class=\"create-account button button--primary\" href=\"{{urls.auth.create_account}}\">{{lang 'login.new_customer.create_account'}}</a>\r\n\t\t                </div>\r\n\t\t            </div>\r\n\t\t        </form>\r\n\t\t    </div>\r\n\t\t</div>\r\n\t{{/if}}\r\n{{/if}}\r\n\r\n{{#if page_type '!=' 'cart'}}\r\n    <div class=\"halo-sidebar halo-cart-sidebar halo-sidebar-right\" id=\"halo-cart-sidebar\">\r\n    \t<div class=\"halo-sidebar-header\">\r\n    \t\t<span class=\"title\">{{lang 'cart.cart_preview'}}</span>\r\n    \t\t<a href=\"#\" class=\"close\">\r\n    \t\t\t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.close'}}\">\r\n                \t<title>{{lang 'common.close'}}</title>\r\n                \t<desc>{{lang 'common.close'}}</desc>\r\n                \t<use xlink:href=\"#icon-close\"></use>\r\n                </svg>\r\n    \t\t</a>\r\n    \t</div>\r\n        <div class=\"halo-sidebar-wrapper haloCustomScrollbar\">\r\n            {{> components/common/cart-preview}}\r\n        </div>\r\n    </div>\r\n{{/if}}\r\n\r\n<div class=\"halo-sidebar halo-menu-sidebar halo-sidebar-right{{#if theme_settings.haloMegamenu}} halo-menu-megamenu{{/if}}\" id=\"halo-menu-sidebar\">\r\n\t<!-- <div class=\"halo-sidebar-header\">\r\n\t\t<span class=\"title\">{{lang 'common.menu'}}</span>\r\n\t    <a href=\"#\" class=\"halo-sidebar-close\">\r\n\t        <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.close'}}\">\r\n\t        \t<title>{{lang 'common.close'}}</title>\r\n\t        \t<desc>{{lang 'common.close'}}</desc>\r\n\t        \t<use xlink:href=\"#icon-close\"></use>\r\n\t        </svg>\r\n\t    </a>\r\n\t</div> -->\r\n    <div class=\"halo-sidebar-wrapper navPages-container\" data-menu>\r\n        {{> components/common/navigation-mobile}}\r\n    </div>\r\n</div>\r\n\r\n<div class=\"halo-sidebar halo-search-sidebar halo-sidebar-right haloCustomScrollbar\" id=\"halo-search-sidebar\">\r\n    <div class=\"halo-sidebar-wrapper\">\r\n    \t<div class=\"halo-sidebar-search\"></div>\r\n    \t<div class=\"halo-sidebar-content\"></div>\r\n    </div>\r\n</div>\r\n\r\n<div id=\"halo-live-help\" class=\"halo-liveHelp halo-live-help halo-liveHelp-top\">\r\n\t<div class=\"halo-sidebar-header\">\r\n        <a href=\"#\" class=\"close\">\r\n            <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.close'}}\">\r\n                <title>{{lang 'common.close'}}</title>\r\n                <desc>{{lang 'common.close'}}</desc>\r\n                <use xlink:href=\"#icon-close\"></use>\r\n            </svg>\r\n        </a>\r\n    </div>\r\n    <div class=\"wrapper halo-liveHelp-wrapper\">\r\n        <div class=\"halo-liveHelp-ask item-top\">\r\n            <h4>{{theme_settings.haloLiveHelp-heading-1}}</h4>\r\n            <p class=\"desc\">{{{theme_settings.haloLiveHelp-desc1}}}</p>\r\n            <a class=\"button button--tertiary ask-an-expert-link\" href=\"#\" title=\"{{lang 'common.ask_an_expert'}}\" aria-label=\"{{lang 'common.ask_an_expert'}}\">\r\n                <svg class=\"icon\"><use xlink:href=\"#icon-headphone\" /></svg><span class=\"text\">{{lang 'common.ask_an_expert'}}</span>\r\n            </a>\r\n            <span class=\"text\">{{{theme_settings.haloLiveHelp-desc2}}}</span>\r\n        </div>\r\n        <div class=\"halo-liveHelp-contact item-bottom\">\r\n            <h4>{{theme_settings.haloLiveHelp-heading-2}}</h4>\r\n            <p class=\"desc\">{{{theme_settings.haloLiveHelp-desc3}}}</p>\r\n            <a class=\"button button--tertiary contact-us-link\" href=\"{{theme_settings.haloLiveHelp-buttonLink}}\" aria-label=\"Contact Us\">\r\n                <svg class=\"icon\"><use xlink:href=\"#icon-email\" /></svg><span class=\"text\">{{theme_settings.haloLiveHelp-button}}</span>\r\n            </a>\r\n        </div>\r\n    </div>\r\n</div>\r\n\r\n<div id=\"halo-lookbook-popup\" class=\"lookbook-popup\">\r\n\t<div class=\"lookbook-popup-wrapper\">\r\n\t\t<div class=\"lookbook-popup-title\">\r\n\t\t\t<h5 class=\"title\"></h5>\r\n\t\t\t<a href=\"#\" class=\"close\">\r\n\t\t\t    <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.close'}}\">\r\n\t\t\t        <title>{{lang 'common.close'}}</title>\r\n\t\t\t        <desc>{{lang 'common.close'}}</desc>\r\n\t\t\t        <use xlink:href=\"#icon-close\"></use>\r\n\t\t\t    </svg>\r\n\t\t\t</a>\r\n\t\t</div>\r\n\t\t<div class=\"lookbook-popup-product haloCustomScrollbar-x\">\r\n\t\t\t<div class=\"lookbook-popup-content\">\r\n\t\t\t\t<div class=\"no-products text-center\" style=\"display: none;\">\r\n                    <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.sorry_no_products'}}\">\r\n                    \t<title>{{lang 'common.sorry_no_products'}}</title>\r\n            \t\t\t<desc>{{lang 'common.sorry_no_products'}}</desc>\r\n                    \t<use xlink:href=\"#icon-no-products\"></use>\r\n                    </svg>\r\n                    <div class=\"text\">{{lang 'common.sorry_no_products'}}</div>\r\n                </div>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t</div>\r\n</div>",
  "components/common/cart-preview": "<div class=\"previewCart\">\r\n    {{#and (if theme_settings.halo_shipping) (if cart.items.length)}}\r\n        <div class=\"halo-free-shipping-message animated-loading\">\r\n            <div class=\"shipping-message\">\r\n                <span class=\"message\"></span>\r\n                <!-- <span class=\"progress-percent\">0%</span> -->\r\n            </div>\r\n            <div class=\"progress-shipping\" role=\"progressbar\">\r\n                <div class=\"progress-meter\" style=\"width: 0\"></div>\r\n            </div>\r\n        </div>\r\n    {{/and}}\r\n    {{#if cart.items.length}}\r\n        <div class=\"previewCartHeader\">\r\n            <span class=\"text\">\r\n                <span>{{lang 'cart.items' NUM=cart.quantity}}</span>\r\n            </span>\r\n        </div>\r\n    {{/if}}\r\n    {{#if cart.items.length}}\r\n        <ul class=\"previewCartList\" data-cart-quantity=\"{{cart.quantity}}\">\r\n            {{#each cart.items}}\r\n                <li class=\"previewCartItem\" {{#if options}}data-cart-edit-productItem{{/if}}>\r\n                    <div class=\"previewCartItem-image\">\r\n                        {{#if type '==' 'GiftCertificate'}}\r\n                            <img src=\"{{cdn ../../theme_settings.default_image_gift_certificate}}\" alt=\"GiftCertificate\" title=\"GiftCertificate\">\r\n                        {{else}}\r\n                            {{> components/common/responsive-img\r\n                                image=image\r\n                                fallback_size=../../theme_settings.productcartthumb_size\r\n                                lazyload=../../theme_settings.lazyload_mode\r\n                                default_image=../../theme_settings.default_image_product\r\n                            }}\r\n                        {{/if}}\r\n                    </div>\r\n                    <div class=\"previewCartItem-content\">\r\n                        <h6 class=\"previewCartItem-name\">\r\n                            {{#if ../theme_settings.halo_card_title '==' 'ellipsis'}}\r\n                                <a href=\"{{url}}\" alt=\"{{name}}\" title=\"{{name}}\" style=\"-webkit-box-orient: vertical;\">\r\n                                    <span>{{name}}</span>\r\n                                </a>\r\n                            {{else}}\r\n                                <a href=\"{{url}}\" alt=\"{{name}}\" title=\"{{name}}\">\r\n                                    <span>{{name}}</span>\r\n                                </a>\r\n                            {{/if}}\r\n                        </h6>\r\n                        {{#and (if brand.name) (if ../theme_settings.halo_card_brand)}}\r\n                            <p class=\"card-text\" data-test-info-type=\"brandName\">{{brand.name}}</p>\r\n                        {{/and}}\r\n                        {{#if options}}\r\n                            <div class=\"previewCartItem-options\">\r\n                                <span class=\"previewCartItem-variant\">\r\n                                    {{#each options}}\r\n                                        <span>{{{ sanitize value}}}</span>\r\n                                    {{/each}}\r\n                                </span>\r\n                                {{#if ../theme_settings.halo_QuickEditCart}}\r\n                                    <a href=\"#\" class=\"previewCartItem-edit\"  data-cart-edit-id=\"{{product_id}}\" data-cart-itemid=\"{{id}}\" data-cart-edit-price=\"{{price.formatted}}\" data-cart-edit-quantity=\"{{quantity}}\">\r\n                                        <svg class=\"icon\"><use xlink:href=\"#icon-edit\"></use></svg>\r\n                                    </a>\r\n                                {{/if}}\r\n                            </div>\r\n                        {{/if}}\r\n                        <div class=\"previewCartItem-wrapper\">\r\n                            <div class=\"previewCartItem-qty\">\r\n                                <label class=\"form-label\" for=\"qty-{{id}}\">{{lang 'products.quantity'}}</label>\r\n                                <div class=\"form-increment\">\r\n                                    <button class=\"button button--icon\" data-cart-update data-cart-itemid=\"{{id}}\" data-action=\"dec\">\r\n                                        <span class=\"is-srOnly\">{{lang 'products.quantity_decrease'}}</span>\r\n                                    </button>\r\n                                    <input class=\"form-input form-input--incrementTotal\"\r\n                                        id=\"qty-{{id}}\"\r\n                                        name=\"qty-{{id}}\"\r\n                                        type=\"number\"\r\n                                        value=\"{{quantity}}\"\r\n                                        data-quantity-min=\"{{min_purchase_quantity}}\"\r\n                                        data-quantity-max=\"{{max_purchase_quantity}}\"\r\n                                        data-quantity-min-error=\"{{lang 'products.quantity_min' quantity=min_purchase_quantity}}\"\r\n                                        data-quantity-max-error=\"{{lang 'products.quantity_max' quantity=max_purchase_quantity}}\"\r\n                                        min=\"1\"\r\n                                        pattern=\"[0-9]*\"\r\n                                        data-cart-itemid=\"{{id}}\"\r\n                                        data-action=\"manualQtyChange\"\r\n                                        aria-live=\"polite\"{{#unless can_modify}} disabled{{/unless}}\r\n                                        >\r\n                                    <button class=\"button button--icon\" data-cart-update data-cart-itemid=\"{{id}}\" data-action=\"inc\">\r\n                                        <span class=\"is-srOnly\">{{lang 'products.quantity_increase'}}</span>\r\n                                    </button>\r\n                                </div>\r\n                            </div>\r\n                            <span class=\"previewCartItem-price\">\r\n                                {{#or ../customer (if ../theme_settings.restrict_to_login '!==' true)}}\r\n                                    <span{{#if price_discounted}} class=\"price--discounted\"{{/if}}>{{price.formatted}}</span>\r\n                                    {{#if price_discounted}}\r\n                                        {{price_discounted.formatted}}\r\n                                    {{/if}}\r\n                                {{else}}\r\n                                    {{> components/common/login-for-pricing}}\r\n                                {{/or}}\r\n                            </span>\r\n                        </div>\r\n                        \r\n                        {{#or can_modify (if type '==' 'GiftCertificate')}}\r\n                            <button class=\"previewCartItem-remove icon\"\r\n                                data-cart-itemid=\"{{id}}\"\r\n                                data-confirm-delete=\"{{lang 'cart.confirm_delete'}}\"\r\n                                aria-label=\"{{lang 'cart.remove_item' name=name}}\"\r\n                            >\r\n                                <svg class=\"icon\"><use xlink:href=\"#icon-close\"></use></svg>\r\n                            </button>\r\n                        {{/or}}\r\n                    </div>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n        <ul class=\"previewCartTotalsPrice\">\r\n            <!-- <li class=\"previewCartTotals subTotal\">\r\n                <div class=\"previewCartTotals-label\">\r\n                    {{lang 'cart.checkout.subtotal'}}:\r\n                </div>\r\n                <div class=\"previewCartTotals-value\">\r\n                    {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                        {{cart.sub_total.formatted}}\r\n                    {{else}}\r\n                        {{> components/common/login-for-pricing}}\r\n                    {{/or}}\r\n                </div>\r\n            </li> -->\r\n            <li class=\"previewCartTotals grandTotal\">\r\n                <div class=\"previewCartTotals-label\">\r\n                    {{lang 'cart.checkout.grand_total'}}:\r\n                </div>\r\n                <div class=\"previewCartTotals-value cart-total-grandTotal\">\r\n                    {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                        <span>{{cart.grand_total.formatted}}</span>\r\n                    {{else}}\r\n                        {{> components/common/login-for-pricing}}\r\n                    {{/or}}\r\n                </div>\r\n            </li>\r\n        </ul>\r\n        <div class=\"previewCartAction\">\r\n            <div class=\"previewCartAction-viewCart\">\r\n                <a href=\"{{urls.cart}}\" class=\"button button--primary \">\r\n                    {{lang 'cart.preview.view_cart'}}\r\n                </a>\r\n            </div>\r\n\r\n            {{#if cart.show_multiple_address_shipping}}\r\n                <div class=\"previewCartAction-checkoutMultiple\">\r\n                    <a href=\"{{urls.checkout.multiple_address}}\">\r\n                        {{lang 'cart.preview.checkout_multiple'}}\r\n                    </a>\r\n                </div>\r\n            {{/if}}\r\n            \r\n            {{#if cart.show_primary_checkout_button}}\r\n                <div class=\"previewCartAction-checkout\">\r\n                    <a href=\"{{urls.checkout.single_address}}\" class=\"button button--checkout\">\r\n                        {{lang 'cart.preview.checkout_now'}}\r\n                    </a>\r\n                </div>\r\n            {{/if}}\r\n        </div>\r\n    {{else}}\r\n        <div class=\"previewCart-emptyBody\">\r\n            {{lang 'cart.checkout.empty_cart'}}\r\n        </div>\r\n    {{/if}}\r\n</div>\r\n",
