alo-byl-history}}\r\n\t\t\t            <div class=\"tab-content{{#if theme_settings.halo-byl-recommended '==' false}} is-active{{/if}}\" id=\"tab-history\">\r\n\t\t\t\t            <div id=\"history-products-list-tmp\" class=\"tab-content--product history-products-list-tmp\">\r\n\t\t\t\t                <div class=\"no-products text-center\">\r\n\t\t\t                        <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.sorry_no_products'}}\">\r\n\t\t\t                        \t<title>{{lang 'common.sorry_no_products'}}</title>\r\n                            \t\t\t<desc>{{lang 'common.sorry_no_products'}}</desc>\r\n\t\t\t                        \t<use xlink:href=\"#icon-no-products\"></use>\r\n\t\t\t                        </svg>\r\n\t\t\t                        <div class=\"text\">{{lang 'common.sorry_no_products'}}</div>\r\n\t\t\t                    </div>\r\n\t\t\t                    <div class=\"productGrid halo-dots halo-arrows-custom\"></div>\r\n\t\t\t\t            </div> \r\n\t\t\t            </div>\r\n\t\t\t        {{/if}}\r\n\t\t        </div>\r\n\t\t\t</div>\r\n\r\n\t\t\t<div class=\"back-to-store\">\r\n\t\t\t\t<a class=\"button button--primary\" href=\"/\">{{lang 'common.back_to_store'}}</a>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t</section>\r\n{{/if}}\r\n",
