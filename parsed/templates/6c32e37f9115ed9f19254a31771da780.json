  "components/halothemes/global/halo-before-you-leave": "{{#if theme_settings.halo_before_you_leave}}\r\n\t<section id=\"before-you-leave\" class=\"before-you-leave side_popup\" data-time=\"{{theme_settings.halo-byl-time}}\" style=\"display: none;\">\r\n        {{> components/halothemes/sidebar/halo-button-close}}\r\n        <div class=\"before-you-leave-inner haloCustomScrollbar\">\r\n\t\t\t<div class=\"logo\">\r\n\t\t\t\t<a href=\"/\">\r\n                    {{#if settings.store_logo.image}}\r\n                        <img src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n                    {{else}}\r\n                        <span>{{settings.store_logo.title}}</span>\r\n                    {{/if}}\r\n                </a>\r\n\t\t\t</div>\r\n\r\n\t\t\t<div class=\"before-you-leave-banner\">\r\n\t\t\t\t<div class=\"banner-inner \">\r\n\t\t\t\t\t{{#if theme_settings.halo-byl-img '!=' ''}}\r\n\t\t\t\t\t\t<a class=\"image-wrapper\" href=\"{{theme_settings.halo-byl-link}}\" title=\"{{theme_settings.halo-byl-title}}\">\r\n\t\t\t\t\t\t\t<img class=\"lazyload\" src=\"\" data-src=\"https://cdn11.bigcommerce.com/s-{{settings.store_hash}}/product_images/uploaded_images/{{theme_settings.halo-byl-img}}\" alt=\"\">\r\n\t\t\t\t\t\t</a>\r\n\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t<div class=\"content text-center{{#unless theme_settings.halo-byl-img}} noImage{{/unless}}\">\r\n\t\t\t\t\t\t<div class=\"content-inner\">\r\n\t\t\t\t\t\t\t{{#if theme_settings.halo-byl-title '!=' ''}}\r\n\t\t\t\t\t\t\t\t<h2 class=\"title\">{{theme_settings.halo-byl-title}}</h2>\r\n\t\t\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t\t\t{{#if theme_settings.halo-byl-text-1 '!=' ''}}\r\n\t\t\t\t\t\t\t\t<p class=\"desc1\">{{theme_settings.halo-byl-text-1}}</p>\r\n\t\t\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t\t\t{{#if theme_settings.halo-byl-heading-1 '!=' ''}}\r\n\t\t\t\t\t\t\t\t<h3 class=\"heading1\"><span>{{theme_settings.halo-byl-heading-1}}</span></h3>\r\n\t\t\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t\t\t{{#if theme_settings.halo-byl-text-2 '!=' ''}}\r\n\t\t\t\t\t\t\t\t<p class=\"desc2\">{{theme_settings.halo-byl-text-2}}</p>\r\n\t\t\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t\t\t{{#if theme_settings.halo-byl-heading-2 '!=' ''}}\r\n\t\t\t\t\t\t\t\t<h3 class=\"heading2\"><span>{{theme_settings.halo-byl-heading-2}}</span></h3>\r\n\t\t\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t{{#if theme_settings.halo-byl-btn-text '!=' ''}}\r\n\t\t\t\t\t\t\t<a class=\"button button--secondary before-you-leave-continue\" href=\"javascript:void(0)\" title=\"{{theme_settings.halo-byl-btn-text}}\">\r\n\t\t\t\t\t\t\t\t<span>{{theme_settings.halo-byl-btn-text}}</span>\r\n\t\t\t\t\t\t\t</a>\r\n\t\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\r\n\t\t\t<div class=\"before-you-leave-tab\">\r\n\t\t\t\t<div class=\"tabs-wrapper\">\r\n\t\t\t\t\t<ul class=\"tabs\" data-tab>\r\n\t\t\t            {{#if theme_settings.halo-byl-recommended}}\r\n\t\t\t                <li class=\"tab is-active recommended\" data-id=\"recommended\">\r\n\t\t\t                    <a class=\"tab-title\" href=\"#tab-recommended\" aria-label=\"{{lang 'common.recommended'}}\">{{lang 'common.recommended'}} <span class=\"count\">0</span></a>\r\n\t\t\t                </li>\r\n\t\t\t            {{/if}}\r\n\t\t\t            {{#if theme_settings.halo-byl-history}}\r\n\t\t\t                <li class=\"tab history{{#if theme_settings.halo-byl-recommended '==' false}} is-active{{/if}}\" data-id=\"history\">\r\n\t\t\t                \t<a class=\"tab-title\" href=\"#tab-history\" aria-label=\"{{lang 'common.history' }}\">{{lang 'common.history' }} <span class=\"count\">0</span></a>\r\n\t\t\t                </li>\r\n\t\t\t            {{/if}}\r\n\t\t\t        </ul>\r\n\t\t\t        {{#if theme_settings.halo-byl-search}}\r\n\t\t\t\t        <div class=\"before-you-leave-search\">\r\n\t\t\t            \t<a class=\"search-icon\" href=\"#\" aria-label=\"Quick Search\" data