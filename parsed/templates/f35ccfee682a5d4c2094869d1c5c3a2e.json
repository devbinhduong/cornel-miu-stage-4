{
  "components/halothemes/options-sticky-cart/input-checkbox": "<div class=\"form-field\" data-product-attribute=\"input-checkbox\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{display_name}}\r\n        {{> components/common/requireness-msg}}\r\n    </label>\r\n    <input type=\"hidden\" name=\"attribute[{{id}}]\" value=\"{{noValue}}\" />\r\n    <input\r\n        class=\"form-checkbox\"\r\n        type=\"checkbox\"\r\n        name=\"attribute[{{id}}]\"\r\n        id=\"st_attribute-check-{{id}}\"\r\n        value=\"{{value}}\"\r\n        {{#if checked}}\r\n            checked\r\n            data-default\r\n        {{/if}}\r\n        {{#if required}}required{{/if}}>\r\n\r\n    <label class=\"form-label {{class}}\" for=\"st_attribute-check-{{id}}\">{{label}}</label>\r\n</div>\r\n",
  "components/common/requireness-msg": "<small>\r\n    {{#if required}}*{{/if}}\r\n</small>\r\n"
}