{
  "components/halothemes/product/halo-product-tab-by-category": "<div class=\"halo-block halo-block-product halo-block-product-tabs padding-top-30 padding-bottom-50\">\r\n    <div class=\"container\">\r\n        <div class=\"productCarousel-tabs\">\r\n            <ul class=\"tabs haloCustomScrollbar-x disable-srollbar\" data-tab role=\"tablist\">\r\n                {{#each (limit categories theme_settings.home_product-id-tab-cate-limit)}}\r\n                    {{#if @index '==' 0}}\r\n                        <li class=\"tab is-active\" role=\"presentation\">\r\n                            <a class=\"tab-title\" href=\"#category_{{id}}\" role=\"tab\" tabindex=\"0\" aria-selected=\"true\" aria-controls=\"category_{{id}}\">\r\n                                <span>{{name}}</span>\r\n                            </a>\r\n                        </li>\r\n                    {{else}}\r\n                        <li class=\"tab\" role=\"presentation\">\r\n                            <a class=\"tab-title\" href=\"#category_{{id}}\" role=\"tab\" tabindex=\"0\" aria-selected=\"false\" aria-controls=\"category_{{id}}\">\r\n                                <span>{{name}}</span>\r\n                            </a>\r\n                        </li>\r\n                    {{/if}}\r\n                {{/each}}\r\n            </ul>\r\n            <div class=\"tabs-contents\">\r\n                {{#each (limit categories theme_settings.home_product-id-tab-cate-limit)}}\r\n                    {{#if @index '==' 0}}\r\n                        <div class=\"tab-content is-active\" id=\"category_{{id}}\" role=\"tabpanel\" aria-hidden=\"false\" data-tab-category-id=\"{{id}}\" data-tab-category-url=\"{{concat url ../data-sort-tab}}\">\r\n                            <div class=\"loadingOverlay\"></div>\r\n                            <div class=\"productCarousel\">\r\n                                {{> components/halothemes/product/halo-sample-product-2}}\r\n                                {{> components/halothemes/product/halo-sample-product-2}}\r\n                                {{> components/halothemes/product/halo-sample-product-2}}\r\n                                {{> components/halothemes/product/halo-sample-product-2}}\r\n                                {{> components/halothemes/product/halo-sample-product-2}}\r\n                            </div>\r\n                        </div>\r\n                    {{else}}\r\n                        <div class=\"tab-content\" id=\"category_{{id}}\" role=\"tabpanel\" aria-hidden=\"true\" data-tab-category-id=\"{{id}}\" data-tab-category-url=\"{{concat url ../data-sort-tab}}\">\r\n                            <div class=\"loadingOverlay\"></div>\r\n                            <div class=\"productCarousel\">\r\n                                {{> components/halothemes/product/halo-sample-product-2}}\r\n                                {{> components/halothemes/product/halo-sample-product-2}}\r\n                                {{> components/halothemes/product/halo-sample-product-2}}\r\n                                {{> components/halothemes/product/halo-sample-product-2}}\r\n                                {{> components/halothemes/product/halo-sample-product-2}}\r\n                            </div>\r\n                        </div>\r\n                    {{/if}}\r\n                {{/each}}\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\r\n{{{region name=\"home_below_products_tab_by_category\"}}}\r\n",
  "components/halothemes/product/halo-sample-product-2": "<div class=\"productCarousel-slide product-sample\">\r\n    <article class=\"card card-sample\">\r\n        <figure class=\"card-figure\">\r\n            <a href=\"#\" class=\"card-link\" aria-label=\"Sample Card\">\r\n                <div class=\"card-img-container animated-loading\"></div>\r\n            </a>\r\n        </figure>\r\n        <div class=\"card-body\">\r\n            <h3 class=\"card-title animated-loading\">\r\n                <a aria-label=\"Sample Card Title\" href=\"#\"><span>Sample Card</span></a>\r\n            </h3>\r\n\r\n            <div class=\"card-wrapper animated-loading\">\r\n                <div class=\"card-price\" data-test-info-type=\"price\">\r\n                    <span class=\"price\">$000.00</span>\r\n                </div>\r\n            </div>\r\n\r\n            <div class=\"card-option animated-loading\">\r\n                <div class=\"form-field\"></div>\r\n            </div>\r\n\r\n            {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                {{#if show_cart_action}}\r\n                    <div class=\"card-figcaption-bottom animated-loading\">\r\n                        <a href=\"#\" data-event-type=\"product-click\" class=\"button button--primary card-figcaption-button\">Add To Cart</a>\r\n                    </div>\r\n                {{/if}}\r\n            {{/or}}\r\n        </div>\r\n    </article>\r\n</div>\r\n"
}