{
  "components/brand/show-more": "{{#if brand.facets}}\r\n    {{> components/faceted-search/show-more-facets brand}}\r\n{{/if}}\r\n",
  "components/faceted-search/show-more-facets": "<div class=\"modal-header\">\r\n    <h2 class=\"modal-header-title facet-quick-heading\">{{all_facets}}</h2>\r\n</div>\r\n<div class=\"modal-body\">\r\n    <div class=\"form-field\" id=\"facetedSearch-filterItems\">\r\n        <input class=\"form-input\" type=\"search\" placeholder=\"{{lang 'forms.search.search'}}\">\r\n    </div>\r\n    <ul class=\"navList facetedSearch-optionColumns\">\r\n        {{#each facets}}\r\n            {{#if facet '===' ../all_facets}}\r\n                {{#each items}}\r\n                    <li class=\"navList-item\">\r\n                        <a\r\n                        href=\"{{url}}\"\r\n                        class=\"navList-action{{#if selected}} is-active{{/if}}\"\r\n                        rel=\"nofollow\"\r\n                        data-id=\"{{id}}\"\r\n                        data-faceted-search-facet\r\n                        >\r\n                            <span class=\"text\">\r\n                                {{{title}}}\r\n                                {{#if ../show_product_counts}}\r\n                                    {{#if count}}\r\n                                        <span>({{count}})</span>\r\n                                    {{/if}}\r\n                                {{/if}}\r\n                            </span>\r\n                        </a>\r\n                    </li>\r\n                {{/each}}\r\n            {{/if}}\r\n        {{/each}}\r\n    </ul>\r\n</div>\r\n"
}