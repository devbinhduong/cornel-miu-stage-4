{
  "components/halothemes/gallery/halo-toolbar": "<div class=\"toolbar-wrapper\">\r\n\t<div class=\"toolbar-left\">\r\n\t\t<div class=\"pagination-info\"></div>\r\n\t</div>\r\n\t<div class=\"toolbar-right\">\r\n\t    {{#if theme_settings.haloStickyToolbar_limit}}\r\n\t\t\t{{> components/halothemes/gallery/halo-show-products limit=limit}}\r\n\t\t{{/if}}\r\n\t\t{{#if theme_settings.haloStickyToolbar_sort}}\r\n\t    \t{{> components/products/filter sort=pagination.category.sort}} \r\n\t    {{/if}}\r\n\t    {{#if theme_settings.haloStickyToolbar_viewAs}}\r\n    \t\t{{> components/halothemes/gallery/halo-view-as}}\r\n\t    {{/if}}\r\n\t</div>\r\n</div>\r\n{{#if theme_settings.haloStickyToolbar_viewAs}}\r\n\t<div class=\"toolbar-wrapper toolbar-mobile\">\r\n\t    <div class=\"toolbar-item\">\r\n\t        {{> components/halothemes/gallery/halo-view-as-2}}  \r\n\t    </div>\r\n\t</div>\r\n{{/if}}\r\n",
  "components/halothemes/gallery/halo-show-products": "<form class=\"actionBar\" method=\"get\" data-sort-by data-show-products>\r\n    <div class=\"form-field\">\r\n        <label class=\"form-label\" for=\"limit\">{{lang 'common.show_products_label'}}</label>\r\n        <select class=\"form-select\" name=\"limit\" id=\"limit\">\r\n            <option value=\"9\" {{#if '9' '==' theme_settings.categorypage_products_per_page }}selected{{/if}}>9 {{lang 'common.show_products'}}</option>\r\n            <option value=\"12\" {{#if '12' '==' theme_settings.categorypage_products_per_page }}selected{{/if}}>12 {{lang 'common.show_products'}}</option>\r\n            <option value=\"15\" {{#if '15' '==' theme_settings.categorypage_products_per_page }}selected{{/if}}>15 {{lang 'common.show_products'}}</option>\r\n            <option value=\"18\" {{#if '18' '==' theme_settings.categorypage_products_per_page }}selected{{/if}}>18 {{lang 'common.show_products'}}</option>\r\n            <option value=\"21\" {{#if '21' '==' theme_settings.categorypage_products_per_page }}selected{{/if}}>21 {{lang 'common.show_products'}}</option>\r\n            <option value=\"24\" {{#if '24' '==' theme_settings.categorypage_products_per_page }}selected{{/if}}>24 {{lang 'common.show_products'}}</option>\r\n            <option value=\"27\" {{#if '27' '==' theme_settings.categorypage_products_per_page }}selected{{/if}}>27 {{lang 'common.show_products'}}</option>\r\n            <option value=\"30\" {{#if '30' '==' theme_settings.categorypage_products_per_page }}selected{{/if}}>30 {{lang 'common.show_products'}}</option>\r\n        </select>\r\n    </div>\r\n</form>\r\n",
  "components/products/filter": "<form class=\"actionBar\" method=\"get\" data-sort-by data-show-by>\r\n    {{> components/category/sort-box sort=sort}}\r\n</form>\r\n",
  "components/halothemes/gallery/halo-view-as-2": "<div class=\"view-as-btn view-as-btn-mobile\">\r\n    <a href=\"javascript:void(0);\" class=\"btn-view grid-view{{#if theme_settings.product_list_display_mode '===' 'grid'}} current-view{{/if}}\" id=\"grid-view-mobile\">\r\n        <svg class=\"icon\"><use xlink:href=\"#icon-grid-view\"></use></svg>\r\n    </a>\r\n    <a href=\"javascript:void(0);\" class=\"btn-view list-view{{#if theme_settings.product_list_display_mode '===' 'list'}} current-view{{/if}}\" id=\"list-view-mobile\">\r\n        <svg class=\"icon\"><use xlink:href=\"#icon-list-view\"></use></svg>\r\n    </a>\r\n</div>\r\n",
  "components/halothemes/gallery/halo-view-as": "<div class=\"view-as-btn\">\r\n    <label class=\"form-label\">{{lang 'common.view_as'}}</label>\r\n    <div class=\"form-group\">\r\n        <a href=\"javascript:void(0);\" class=\"btn-view grid-view{{#if theme_settings.product_list_display_mode '===' 'grid'}} current-view{{/if}}\" id=\"grid-view\" aria-label=\"{{lang 'ada.grid_view'}}\">\r\n            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-grid-view\"></use></svg>\r\n        </a>\r\n        <a href=\"javascript:void(0);\" class=\"btn-view list-view{{#if theme_settings.product_list_display_mode '===' 'list'}} current-view{{/if}}\" id=\"list-view\" aria-label=\"{{lang 'ada.list_view'}}\">\r\n            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-list-view\"></use></svg>\r\n        </a>\r\n    </div>\r\n</div>\r\n",
  "components/category/sort-box": "<fieldset class=\"form-fieldset actionBar-section\">\r\n    <div class=\"form-field\">\r\n        <label class=\"form-label\" for=\"sort\">{{lang 'common.sorter.sort_by'}}</label>\r\n        <select class=\"form-select\" name=\"sort\" id=\"sort\" role=\"listbox\">\r\n            <option value=\"featured\" {{#if sort '==' 'featured'}}selected{{/if}}>{{lang 'common.sorter.featured'}}</option>\r\n            <option value=\"newest\" {{#if sort '==' 'newest'}}selected{{/if}}>{{lang 'common.sorter.newest'}}</option>\r\n            <option value=\"bestselling\" {{#if sort '==' 'bestselling'}}selected{{/if}}>{{lang 'common.sorter.top_sellers'}}</option>\r\n            <option value=\"alphaasc\" {{#if sort '==' 'alphaasc'}}selected{{/if}}>{{lang 'common.sorter.alpha_asc'}}</option>\r\n            <option value=\"alphadesc\" {{#if sort '==' 'alphadesc'}}selected{{/if}}>{{lang 'common.sorter.alpha_desc'}}</option>\r\n            <option value=\"avgcustomerreview\" {{#if sort '==' 'avgcustomerreview'}}selected{{/if}}>{{lang 'common.sorter.reviews'}}</option>\r\n            <option value=\"priceasc\" {{#if sort '==' 'priceasc'}}selected{{/if}}>{{lang 'common.sorter.price_asc'}}</option>\r\n            <option value=\"pricedesc\" {{#if sort '==' 'pricedesc'}}selected{{/if}}>{{lang 'common.sorter.price_desc'}}</option>\r\n            {{#if result_count}}\r\n                <option value=\"relevance\" {{#if sort '==' 'relevance'}}selected{{/if}}>{{lang 'common.sorter.relevance'}}</option>\r\n            {{/if}}\r\n        </select>\r\n    </div>\r\n</fieldset>\r\n"
}