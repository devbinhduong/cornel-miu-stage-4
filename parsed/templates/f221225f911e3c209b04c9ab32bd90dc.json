{
  "components/faceted-search/facets/multi": "{{#if items}}\r\n<div class=\"accordion-block\">\r\n    {{> components/faceted-search/faceted-search-navigation}}\r\n\r\n    <div id=\"facetedSearch-content--{{dashcase facet}}\" class=\"accordion-content{{#unless start_collapsed}} is-open{{/unless}}\">\r\n        <ul id=\"facetedSearch-navList--{{dashcase facet}}\" data-facet=\"{{facet}}\" class=\"navList haloCustomScrollbar\" data-has-more-results=\"{{has_more_results}}\">\r\n            {{#each items}}\r\n                <li class=\"navList-item\">\r\n                    <a\r\n                        href=\"{{url}}\"\r\n                        class=\"navList-action navList-action--checkbox{{#if selected}} is-selected{{/if}}\"\r\n                        rel=\"nofollow\"\r\n                        data-faceted-search-facet>\r\n                        <span class=\"text\">\r\n                            {{{sanitize title}}}\r\n                            {{#if ../show_product_counts}}\r\n                                <span>({{count}})</span>\r\n                            {{/if}}\r\n                        </span>\r\n                        <span class=\"navList-action-close\" aria-hidden=\"true\">\r\n                            <svg class=\"icon\">\r\n                                <use xlink:href=\"#icon-close\"/>\r\n                            </svg>\r\n                        </span>\r\n                    </a>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n\r\n        {{#if show_more_toggle}}\r\n            <div class=\"navView-all\">\r\n                <a href=\"#facetedSearch-navList--{{dashcase facet}}\" role=\"button\" class=\"toggleLink toggleLink-viewMore\">\r\n                    <span class=\"toggleLink-text toggleLink-text--off\">{{lang 'search.faceted.show-more'}}</span>\r\n                </a>\r\n            </div>\r\n        {{/if}}\r\n        {{#if selected}}\r\n            <div class=\"navView-clear\">\r\n                <a href=\"{{remove_url}}\" class=\"toggleLink toggleLink-clear facetedSearch-clearLink\" data-faceted-search-facet>\r\n                    <span class=\"toggleLink-text toggleLink-text--off\">{{lang 'search.faceted.clear'}}</span>\r\n                </a>\r\n            </div>\r\n        {{/if}}\r\n    </div>\r\n</div>\r\n{{/if}}\r\n",
  "components/faceted-search/faceted-search-navigation": "<div class=\"accordion-nav-clear-holder\">\r\n    <button type=\"button\"\r\n         class=\"accordion-navigation toggleLink{{#unless start_collapsed}} is-open{{/unless}}\"\r\n         data-collapsible=\"#facetedSearch-content--{{dashcase facet}}\"\r\n         aria-label=\"{{lang 'search.faceted.toggleSection' title=title}}\"\r\n    >\r\n        <span class=\"accordion-title\">\r\n            {{title}}\r\n        </span>\r\n\r\n        <span class=\"accordion-navigation-actions\">\r\n            <svg class=\"icon accordion-indicator toggleLink-text\">\r\n                <use xlink:href=\"#slick-arrow-next\" />\r\n            </svg>\r\n        </span>\r\n    </button>\r\n</div>\r\n"
}