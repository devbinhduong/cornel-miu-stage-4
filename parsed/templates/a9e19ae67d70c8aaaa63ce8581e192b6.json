{
  "components/products/description-tabs-2": "<section class=\"productDescription\">\r\n    <div class=\"productDescription__block\">\r\n        <div class=\"productDescription__title\">\r\n            <h2 class=\"title\">{{lang 'products.description'}}</h2>\r\n        </div>\r\n        <div class=\"productDescription__content\">\r\n            {{{product.description}}}\r\n        </div>\r\n    </div>\r\n</section>\r\n{{#if settings.show_product_reviews}}\r\n    {{#if product.reviews.total}}\r\n        <section class=\"toggle productReviews\" id=\"product-reviews\" data-product-reviews>\r\n            <div class=\"toggle-title\">\r\n                <h4 class=\"page-heading\">{{lang 'products.reviews.header'}}</h4>\r\n                <span class=\"number-review\">{{lang 'products.reviews.number_review' total=product.reviews.total}}</span>\r\n                <a class=\"write-review\" href=\"{{product.url}}{{#if is_ajax}}#write_review{{/if}}\"\r\n                   {{#unless is_ajax }}data-reveal-id=\"modal-review-form\"{{/unless}}>\r\n                   {{lang 'products.reviews.write_a_review'}}\r\n                </a>\r\n                <a class=\"toggleLink\" data-collapsible href=\"#productReviews-content\" aria-expanded=\"false\">\r\n                    <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\"/></svg>\r\n                    <span class=\"toggleLink-text toggleLink-text--on\">\r\n                        {{lang 'products.reviews.hide'}}\r\n                    </span>\r\n                    <span class=\"toggleLink-text toggleLink-text--off\">\r\n                        {{lang 'products.reviews.show'}}\r\n                    </span>\r\n                </a>\r\n            </div>\r\n            <div class=\"toggle-content\" id=\"productReviews-content\" aria-hidden=\"true\">\r\n                <ul class=\"productReviews-list\" id=\"productReviews-list\">\r\n                    {{#each product.reviews.list}}\r\n                    <li class=\"productReview\">\r\n                        <article>\r\n                            <header>\r\n                                <h5 class=\"productReview-title\">{{ title }}</h5>\r\n                                <span class=\"productReview-rating rating--small\">\r\n                                    {{> components/products/ratings rating=rating}}\r\n                                    <span class=\"productReview-ratingNumber\">{{ rating }}</span>\r\n                                </span>\r\n                                {{#if name}}\r\n                                    <p class=\"productReview-author\">\r\n                                        {{lang 'products.reviews.post_on_by' date=date name=name }}\r\n                                    </p>\r\n                                {{else}}\r\n                                    <p class=\"productReview-author\">\r\n                                        {{lang 'products.reviews.post_on_by' date=date name=(lang 'products.reviews.anonymous_poster')}}\r\n                                    </p>\r\n                                {{/if}}\r\n                            </header>\r\n                            <p class=\"productReview-body\">{{nl2br text}}</p>\r\n                        </article>\r\n                    </li>\r\n                    {{/each}}\r\n                </ul>\r\n            </div>\r\n        </section>\r\n    {{/if}}\r\n    {{{snippet 'reviews'}}}\r\n{{/if}}\r\n",
  "components/products/ratings": "<span role=\"img\"\r\n    {{#if rating_target}}\r\n      aria-label=\"{{lang 'products.reviews.rating_aria_label' rating_target=rating_target current_rating=rating max_rating=5}}\"\r\n    {{else}}\r\n      aria-label=\"{{lang 'products.reviews.rating_aria_label' rating_target='Product' current_rating=rating max_rating=5}}\"\r\n    {{/if}}\r\n>\r\n    {{#for 1 5}}\r\n        {{#if ../this.rating '>=' $index}}\r\n            <span class=\"icon icon--ratingFull\">\r\n                <svg>\r\n                    <use xlink:href=\"#icon-star\" />\r\n                </svg>\r\n            </span>\r\n        {{else}}\r\n            <span class=\"icon icon--ratingEmpty\">\r\n                <svg>\r\n                    <use xlink:href=\"#icon-star\" />\r\n                </svg>\r\n            </span>\r\n        {{/if}}\r\n    {{/for}}\r\n</span>\r\n"
}