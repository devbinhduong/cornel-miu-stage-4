{
  "components/carousel-content": "<div class=\"heroCarousel-content{{#unless show_background}} heroCarousel-content--empty{{/unless}} {{#unless hasBackground '==' 'transparent'}} heroCarousel-content--notEmpty{{/unless}} heroCarousel-content--{{position}}\">\r\n    {{#if subheading}}\r\n        <span class=\"heroCarousel-subtitle animated\">{{subheading}}</span>\r\n    {{/if}}\r\n    {{#if heading}}\r\n        <h2 class=\"heroCarousel-title animated\">{{{heading}}}</h2>\r\n    {{/if}}\r\n    {{#if text}}\r\n        <p class=\"heroCarousel-description animated\">{{{text}}}</p>\r\n    {{/if}}\r\n    {{#or (if button_text) (if customTextBtn)}}\r\n        <div class=\"heroCarousel-action animated\">\r\n            {{#if button_text}}\r\n                <a href=\"{{url}}\" aria-label=\"{{#if this.alt_text.length '!=' 0}}{{this.alt_text}}{{else}}{{lang 'carousel.slide_number' slide_number=(add @index 1)}}{{/if}}, {{button_text}}\" class=\"button button--secondary{{#if customTextBtn}} button--custom{{/if}}\">{{button_text}}</a>\r\n            {{/if}}\r\n            {{#if customTextBtn}}\r\n                <a href=\"{{customBtn}}\" aria-label=\"{{#if this.alt_text.length '!=' 0}}{{this.alt_text}}{{else}}{{lang 'carousel.slide_number' slide_number=(add @index 1)}}{{/if}}, {{button_text}}\" class=\"button button--primary button--custom\">{{customTextBtn}}</a>\r\n            {{/if}}\r\n        </div>\r\n    {{/or}}\r\n</div>\r\n\r\n"
}