{
  "components/halothemes/options-sticky-cart/set-radio": "<div class=\"form-field\" data-product-attribute=\"set-radio\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{ this.display_name }}\r\n        {{> components/common/requireness-msg}}\r\n    </label>\r\n    {{#unless required}}\r\n        <input\r\n            class=\"form-radio\"\r\n            type=\"radio\"\r\n            id=\"st_attribute_radio_{{../id}}_none\"\r\n            name=\"attribute[{{../id}}]\"\r\n            value=\"\"\r\n            checked=\"{{#if defaultValue '==' ''}}checked{{/if}}\"\r\n        >\r\n        <label class=\"form-label\" for=\"st_attribute_radio_{{../id}}_none\">{{lang 'products.none'}}</label>\r\n    {{/unless}}\r\n    {{#each this.values}}\r\n        <input\r\n            class=\"form-radio\"\r\n            type=\"radio\"\r\n            id=\"st_attribute_radio_{{../id}}_{{id}}\"\r\n            name=\"attribute[{{../id}}]\"\r\n            value=\"{{id}}\"\r\n            {{#if selected}}\r\n                checked\r\n                data-default\r\n            {{/if}}\r\n            {{#if ../required}}required{{/if}}>\r\n        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"st_attribute_radio_{{../id}}_{{id}}\">{{this.label}}</label>\r\n    {{/each}}\r\n</div>\r\n",
  "components/common/requireness-msg": "<small>\r\n    {{#if required}}*{{/if}}\r\n</small>\r\n"
}